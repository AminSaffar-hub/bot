<!--joints-->
    <joint name="base_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0"/>
        <parent link="base_footprint"/>
        <child link="base_link"/>
    </joint>
    <joint name="fake_laser_joint" type="fixed">
        <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="fake_laser_link"/>
    </joint>
    <joint name="imu_joint" type="fixed">
        <origin xyz="0.01 -0.07 0.0" rpy="0.0 0.0 3.14"/>
        <parent link="base_link"/>
        <child link="imu_link"/>
    </joint>
    <joint name="gps_joint" type="fixed">
        <origin xyz="-0.16 0.0 0.18" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="gps_link"/>
    </joint>
    <joint name="LFW_joint" type="continuous">
        <origin xyz=" 0.0575 0.115 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="RFW_link"/>
        <axis xyz="0.0 1.0 0.0"/>

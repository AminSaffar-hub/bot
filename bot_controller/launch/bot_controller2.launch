<?xml version="1.0" encoding="UTF-8"?>
<launch>
    
    <rosparam file="$(find bot_controller)/config/bot_control2.yaml" command="load"/>

    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" ns="/bot"
    output="screen"  
    args="
    husky_joint_publisher
    husky_velocity_controller
    "/>

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
        <remap from="/joint_states" to="/bot/joint_states"/>
    </node>
</launch>